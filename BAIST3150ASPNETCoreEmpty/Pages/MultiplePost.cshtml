@page
@model BAIST3150ASPNETCoreEmpty.Pages.MultiplePostModel
@{
}
<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <title>Multiple Post</title>
    </head>
    <body>
        <form name="MultiplePostForm" method="post"  >
            <table>
                <tr>
                    <td>First Input Field:</td>
                    <td>
                        <input type="text" name="FirstInputField" />
                    </td>
                </tr>
                <tr>
                    <td>
                        <input type="submit" name="Submit" value="First" />
                    </td>
                </tr>
                <tr>
                    <td>Second Input Field:</td>
                    <td>
                        <input type="text" name="SecondInputField" />
                    </td>
                </tr>
                <tr>
                    <td>
                        <input type="submit" name="Submit" value="Second" />
                    </td>
                </tr>
            </table>
        </form>
        @Model.Message
    </body>
</html>
