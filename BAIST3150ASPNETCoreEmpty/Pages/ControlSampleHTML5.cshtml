@page
@model BAIST3150ASPNETCoreEmpty.Pages.ControlSampleHTML5Model
@{
}
<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <title>Control Smaple HTML5</title>
    </head>
    <body>
        <form name="ControlSampleHTML5Form" method="post" action="~/ViewHTTPRequest">
            <table>
                <tr>
                    <td>Email Address:</td>
                    <td>
                        <input type="email" name="Email" />
                    </td>                 
                </tr>
                <tr>
                    <td>URL:</td>
                    <td>
                        <input type="url" name="URL" />
                    </td>
                </tr>
                <tr>
                    <td> Telephone Number:</td>
                    <td>
                        <input type="tel" name="TelephoneNumber" />
                    </td>
                </tr>
                <tr>
                    <td>Seach:</td>
                    <td>
                        <input type ="search" name="Search" />
                    </td>
                </tr>
                <tr>
                    <td>DataList:</td>
                    <td>
                        <input type="text" name="DataListentry"  list="Datalist" />
                        <datalist id="Datalist" />
                            <option>Red</option>
                            <option>White</option>
                            <option value="Blue"></option>
                         </datalist>
                    </td>
                </tr>
                <tr>
                    <td>Slider:</td>
                    <td>
                        Low(1)<input type="range" name="Slider" min="1" max="5" step="1" /> High(5)
                    </td>
                </tr>
                <tr>
                    <td>Spinner:</td>
                    <td>
                        <input type="number" name="Spinner" min="1" max="5" step="1" />
                    </td>
                </tr>
                <tr>
                    <td>Calendar(date):</td>
                    <td>
                        <input type="date" name="Date" />
                    </td>
                </tr>
                <tr>
                    <td>Calender(datetime):</td>
                    <td>
                        <input type="datetime" name="DateTime" />
                    </td>
                </tr>
                <tr>
                    <td>Calender(datetime-local):</td>
                    <td>
                        <input type="datetime-local" name="DateTimeLocal" />
                    </td>
                 </tr>
                 <tr>
                     <td>Calender(time):</td>
                     <td> 
                         <input type="time" name="Time" /> 
                     </td>
                 </tr>          
                 <tr>
                     <td>Calender(month):</td>
                     <td>
                         <input type="month" name="Month" />
                     </td>
                 </tr>
                 <tr>
                     <td>calender(week):</td>
                     <td>
                         <input type="week" name="Week" />
                     </td>
                 </tr>
                 <tr>
                     <td>Color-Well:</td>
                     <td>
                         <input type="color" name="ColorWell" />
                     </td>
                 </tr>
            <tr>
                    <td>
                        <input type="submit" name="Submit" value="Submit" />
                    </td>
                    
                </tr>
            </table>

        </form>
    </body>
</html>
